<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="683.0" prefWidth="672.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csd214lab2_rajin.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="ssn" text="sn" />
   <TextField fx:id="sfname" text="fname" />
   <TextField fx:id="saddress" text="address" />
   <TextField fx:id="szipcode" text="zipcode" />
    <Button mnemonicParsing="false" text="Insert" onAction="#InsertData" />
    <Button mnemonicParsing="false" text="Update" onAction="#UpdateData" />
    <Button mnemonicParsing="false" text="Delete" onAction="#DeleteData" />
    <Button mnemonicParsing="false" text="LoadData" onAction="#LoadData" />
   <TableView fx:id="detailstable" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn  fx:id="sn" prefWidth="75.0" text="sn" />
       <TableColumn  fx:id="fname" minWidth="0.0" prefWidth="185.5999755859375" text="fname" />
         <TableColumn  fx:id="address" prefWidth="211.20001220703125" text="address" />
         <TableColumn  fx:id="zipcode" prefWidth="158.4000244140625" text="zipcode" />
     </columns>
   </TableView>

    <Label fx:id="welcomeText" />
   <Button onAction="#onHelloButtonClick" mnemonicParsing="false" text="Fetch" />
</VBox>
